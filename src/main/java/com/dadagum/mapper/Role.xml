<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.dadagum.mapper.RoleMapper">
    <select id="getUserRoles" resultType="string">
        SELECT description FROM user_roles NATURAL JOIN roles NATURAL JOIN users WHERE username=#{username}
    </select>
</mapper>